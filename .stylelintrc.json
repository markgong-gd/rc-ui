{
    "extends": [
        "stylelint-config-standard",
        "stylelint-config-recess-order",
        "stylelint-config-prettier"
    ],
    "plugins": [
      "stylelint-order"
    ],
    "rules": {
      "indentation": 2,
      "number-leading-zero": null,
      "unit-case": null,
      "font-family-no-missing-generic-family-keyword": null,
      "no-empty-source": null,
      "color-hex-case": null
    }
  }
  